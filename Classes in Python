class EMP:
    def read(self):
        self.rollno1=int(input("Enter the id "))
        self.name1=input("Enter the name")
        self.marks1=float(input("Enter the data structure "))
        self.marks2=float(input("Enter the oops"))
        self.marks3=float(input("Enter the python "))
    def average(self):
        self.average=(self.marks1+self.marks2+self.marks3)/3
    def display(self):
        print(str(self.rollno1)+" "+self.name1+" "+str(self.average))
a1=EMP()
#a2=EMP()
a1.read()
a1.average()
#a2.read()
a1.display()
