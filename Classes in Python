class EMP:

    def __init__(self,id1,name1,sal):
        self.id1=id1
        self.name1=name1
        self.sal=sal
    def display(self):
        print(str(self.id1)+" "+self.name1+" "+str(self.sal))
a1=EMP(12,"UMAID",200000)
a1.display()
