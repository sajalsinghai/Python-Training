def fact(key):
    temp=1
    for x in range(1,key+1):
        temp=temp*x
    return(temp)
print(fact(3))
6
def prime(a):
    count=0
    for key in range (1,int(a/2)):
        
        if(a%key==0):
            count=count+1
    if(count==1):
        print("PRIME")
    else:
        print("NONPRIME")
prime(13)        
def prime(a):
    count=0
    for key in range (1,int(a/2)):
        
        if(a%key==0):
            count=count+1
    if(count==1):
        print("PRIME")
    else:
        print("NONPRIME")
prime(13)        
