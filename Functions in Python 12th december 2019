def pp(a):
    print("this is called from pp which is called from dispa wh9och is called from display {}",format(a))
def oo(b):
    print("this is called from oo which is called from dispb wh9och is called from display  ",format(b))    
def dispa(a):
    pp(a)
def dispb(b):
    oo(b)    
def display(a,b):
    if a>b:
        dispa(a)
    else:
        dispb(b)
display(123,321)




